{
  "name": "stempeluhr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "diff": "supabase db diff --schema public",
    "db-reset": "supabase db reset",
    "typegen": "supabase gen types typescript --local > src/types/supabase.ts",
    "newmigration": "supabase migration new",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal static/icon.svg"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.4.5",
    "@skeletonlabs/skeleton": "^2.0.0",
    "@skeletonlabs/tw-plugin": "^0.1.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/luxon": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vite-pwa/assets-generator": "^0.0.4",
    "@vite-pwa/sveltekit": "^0.2.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "postcss": "^8.4.25",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "supabase": "^1.77.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-plugin-pwa": "^0.16.4",
    "workbox-window": "^7.0.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.10.1",
    "@supabase/supabase-js": "^2.26.0",
    "@vercel/analytics": "^1.0.1",
    "date-picker-svelte": "^2.6.0",
    "luxon": "^3.3.0"
  }
}
