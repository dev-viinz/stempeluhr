{
  "name": "stempeluhr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "diff": "supabase db diff --schema public",
    "db-reset": "supabase db reset",
    "typegen": "supabase gen types typescript --local > src/types/supabase.ts",
    "newmigration": "supabase migration new",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal static/icon.svg"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@skeletonlabs/skeleton": "^2.3.0",
    "@skeletonlabs/tw-plugin": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/luxon": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vite-pwa/assets-generator": "^0.0.10",
    "@vite-pwa/sveltekit": "^0.2.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "supabase": "^1.106.1",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.5",
    "workbox-window": "^7.0.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.38.2",
    "@vercel/analytics": "^1.1.1",
    "date-picker-svelte": "^2.8.0",
    "luxon": "^3.4.3"
  }
}
